@using Batu.Entites.Concrete
@model List<SocialMediaEntity>
    <div class="social-nav">
        <nav role="navigation">
            <ul class="nav justify-content-center">
                @foreach(var item in Model){
                <li class="nav-item"><a class="nav-link" href="@item.Url" title="@item.Name"><i class="fab fa-@item.Name.ToLower()"></i><span class="menu-title sr-only">@item.Name.ToUpper()</span></a></li>
                }  
                 </ul>
        </nav>
        </div>